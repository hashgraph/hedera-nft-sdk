{
  "name": "@hashgraph/nft-utilities",
  "version": "2.1.1",
  "description": "NFT Utilities for Hedera Hashgraph",
  "author": "Michiel Mulders",
  "license": "Apache License",
  "bugs": {
    "url": "https://github.com/hashgraph/hedera-nft-utilities/issues"
  },
  "homepage": "https://github.com/hashgraph/hedera-nft-utilities#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashgraph/hedera-nft-utilities.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "fix": "eslint . --ext .ts,.js --fix",
    "test": "jest",
    "test-output": "jest --ci --reporters=default --reporters=jest-junit",
    "build": "rm -rf dist/ && npm run fix && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "keywords": [
    "Hedera",
    "Hedera Hashgraph",
    "HIP412",
    "NFT",
    "SDK"
  ],
  "dependencies": {
    "axios": "^1.2.2",
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/src/test/**/*.(spec|test).(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
